                    }
                }
            }
        }
    },
    "foot": {
        "type": "element",
        "elementType": "SegementToolbar",
        "elementProps": {
            "ignoreItems": [
                "New",
                "Close"
            ],
            "captionMapping": {
                "发送PDF文件至邮箱": "封面打印",
                "撤销已提交审批": "撤销"
            }
        }
    },
    "action": {
        "type": "element",
        "elementType": "GridActionButton",
        "elementProps": {
            "yigoid": "Grid1",
            "style": {
                "right": "50%",
                "transform": "translateX(30px)"
            }
        }
    },
    "items": [
        "NO",
        "BillDate",
        "Status",
        "CCID",
        "PersonnelID",
        "CostCompayCodeID",
        "CostDept",
        "Type",
        "StartDate",
        "EndDate",
        "Region",
        "ApplyMoney",
        "HasCost",
        "Balance",
        "Explain",
        "Grid1",
