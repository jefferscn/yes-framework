    "react-wx-images-viewer": "^1.0.6",
    "yes-common": "^2.0.0",
    "yes-comp-react-native-web": "^1.0.2",
    "yes-core": "^3.0.4",
    "yes-intf": "^3.0.6",
    "yes-web": "^3.0.1"
  },
