{
  "name": "mUI",
  "version": "2.0.0",
  "description": "",
  "main": "init.js",
  "scripts": {
    "start": "gulp serve --platform web",
    "build": "npm run build:clean && cross-env NODE_ENV=production webpack --config config/webpack.prod.babel.js --color -p --progress",
    "build:debug": "npm run build:clean && cross-env NODE_ENV=production webpack --config config/webpack.prod.debug.babel.js --color -p --progress",
    "build:release": "npm run build:clean && cross-env NODE_ENV=production webpack --config config/webpack.prod.babel.js --color -p --progress",
    "build:wechat": " cross-env YIGO_PLATFORM=wechat npm run build",
    "build:clean": "rimraf ./build",
    "buildnative": "gulp dist --platform native",
    "startrelease": "gulp startrelease",
    "gulp": "gulp",
    "test": "jest",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "lint:eslint": "eslint --ignore-path .gitignore",
    "lint:js": "npm run lint:eslint -- ./src/components/Canvas ",
    "lint": "npm run lint:js",
    "start:production": "npm run build && npm run start:prod",
    "start:prod": "http-server build -c-1 -g -o",
    "dd": "npm run lint"
  },
  "keywords": [
    "mui"
  ],
  "author": "gmf",
  "license": "ISC",
  "peerDependencies": {
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "dependencies": {
